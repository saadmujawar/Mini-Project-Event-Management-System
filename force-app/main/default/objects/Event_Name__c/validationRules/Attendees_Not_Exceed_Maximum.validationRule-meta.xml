<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Attendees_Not_Exceed_Maximum</fullName>
    <active>true</active>
    <errorConditionFormula>Registered_Attendees__c &gt; Maximum_Attendees__c</errorConditionFormula>
